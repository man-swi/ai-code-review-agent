    {
      "parameters": {
        "jsCode": "const items = $input.all();\n\nfor (const item of items) {\n  // 1. Get the text involved\n  const prompt = item.json.prompt || ''; // The question sent\n  const response = item.json.text || '';   // The AI answer\n\n  // 2. Estimate Tokens (Rule of thumb: 4 chars = 1 token)\n  const promptTokens = Math.ceil(prompt.length / 4);\n  const responseTokens = Math.ceil(response.length / 4);\n  const totalTokens = promptTokens + responseTokens;\n\n  // 3. Estimate Cost (Gemini Flash pricing: ~$0.10 per million tokens)\n  // Input: $0.075/1M | Output: $0.30/1M\n  const inputCost = (promptTokens / 1000000) * 0.075;\n  const outputCost = (responseTokens / 1000000) * 0.30;\n  const totalCost = inputCost + outputCost;\n\n  // 4. Save to the item\n  item.json.tokens = totalTokens;\n  item.json.cost = totalCost.toFixed(6); // Keep 6 decimals (e.g. 0.000450)\n}\n\nreturn items;"
      },
